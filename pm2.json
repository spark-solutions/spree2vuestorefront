{
  "apps": [
    {
      "name": "api",
      "interpreter": "node",
      "script": "./dist/index.js",
      "args": "api-server",
      "log_date_format": "YYYY-MM-DD HH:mm:ss",
      "watch": false
    },
    {
      "name": "cron",
      "script": "yarn run import",
      "cron_restart": "${CRON_SCHEDULE}",
      "autorestart": false,
      "log_date_format": "YYYY-MM-DD HH:mm:ss",
      "watch": false
    }
  ]
}
